class Solution {
public:
    string reverseWords(string s) {
        stack<string> stk;
        string words,ans;
        string::iterator itr,f;
       
        stringstream ss(s);
        
        while(ss>>words)
        {
            cout<<words<<" ";
            stk.push(words+' ');
        }
        while(!stk.empty())
        {
            ans.append(stk.top());
            stk.pop();
        }
return ans;
    }
};
